    "warp-works",
    "warpjs",
    "plugins"
  ],
  "author": {
    "name": "Hữu Ðà Trần",
    "email": "huuda.tran@quoininc.com",
    "url": "https://www.quoininc.com/staff/huu-da-tran/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WarpWorks/warpjs-plugins/issues"
  },
  "homepage": "https://github.com/WarpWorks/warpjs-plugins#readme",
  "dependencies": {
    "debug": "^4.3.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@quoin/eslint-config-quoin": "^2.0.0",
    "@quoin/node-test-helpers": "^2.0.0",
    "eslint": "^7.28.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-json": "^3.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "mocha": "^9.0.1",
    "nyc": "^15.1.0"
  }
