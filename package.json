{
  "name": "@warp-works/warpjs-plugins",
  "version": "1.2.5",
  "description": "WarpJS plugins utilities",
  "main": "index.js",
  "scripts": {
    "prepublish": "npm run -s lint && npm -s test",
    "coverage:unit": "nyc --report-dir=reports/coverage/unit npm run -s test:unit",
    "coverage": "npm run -s coverage:unit",
    "lint:node": "eslint --fix --ignore-pattern \"*.test.js\" *.js lib",
    "lint": "npm run -s lint:node",
    "test:unit": "mocha \"*.unit.test.js\" \"lib/**/*.unit.test.js\"",
    "test": "npm run -s test:unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WarpWorks/warpjs-plugins.git"
  },
  "keywords": [
    "warp-works",
    "warpjs",
    "plugins"
  ],
  "author": "Huu Da",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WarpWorks/warpjs-plugins/issues"
  },
  "homepage": "https://github.com/WarpWorks/warpjs-plugins#readme",
  "dependencies": {
    "lodash": "4.x.x"
  },
  "devDependencies": {
    "@quoin/eslint-config-quoin": "latest",
    "@quoin/node-test-helpers": "latest",
    "eslint": "4.19.x",
    "eslint-plugin-html": "4.0.x",
    "eslint-plugin-import": "2.12.x",
    "eslint-plugin-json": "1.2.x",
    "eslint-plugin-node": "6.0.x",
    "eslint-plugin-promise": "3.8.x",
    "eslint-plugin-standard": "3.1.x",
    "mocha": "5.2.x",
    "nyc": "11.8.x"
  }
}
